Helper = readMM("C:/workspace/DML/test/scripts/applications/univariate/in/helper/in", rows=1, cols=2, format="text");

V = readMM("C:/workspace/DML/test/scripts/applications/univariate/in/vector", rows=10000, cols=1, format="text")
P = readMM("C:/workspace/DML/test/scripts/applications/univariate/in/prob", rows=5, cols=1, format="text")

n = nrow(V)

# sum
s1 = sum(V)

# squared sum
s2 = sum(V^2)

# cubic sum
s3 = sum(V^3)

# quard sum
s4 = sum(V^4)

# mean
mu = s1/n

# standard deviations
std_dev = sqrt((s2/n - mu*mu)*(n/(n-1)))

# standard errors of mean
SE = std_dev/sqrt(n)

# variances
var = std_dev^2

# coefficients of variation
cv = std_dev/mu

# harmonic means (note: may generate out of memory for large sparse matrices becauses of NaNs)
har_mu = n/sum((1.0/V))

# geometric means is not currently supported.
 geom_mu = n*exp(sum(log(V))/n)

# min and max
mn=min(V)
mx=max(V)

# range
rng = mx - mn

# 2nd central moment
m2=var*(n-1)/n

# 4th central moment
m4=(s4-4*mu*s3+6*mu^2*s2-4*mu^3*s1)/n+mu^4

# Skewness
g1 = (s3 - 3*mu*s2 + 3*mu^2*s1 -n*mu^3)/(n*std_dev^3)

# standard error of skewness (not sure how it is defined without the weight)
se_g1=sqrt( 6*n*(n-1.0) / ((n-2.0)*(n+1.0)*(n+3.0)) )

# Kurtosis (using binomial formula)
g2 = (sum(V^4) - 4*s3*mu + 6*s2*mu^2 - 3*n*mu^4)/(n*std_dev^4) - 3

# Standard error of Kurtosis (not sure how it is defined without the weight)
se_g2= sqrt( (4*(n^2-1)*se_g1^2)/((n+5.0)*(n-3.0)) )

# median
md = quantile(V, 0.5)

# quantile
Q = quantile(V, P)

# inter-quartile mean
iqm = interQuartileMean(V)

# outliers use ppred to describe it
out_minus = ppred(V, mu-5*std_dev, "<")*V 
out_plus = ppred(V, mu+5*std_dev, ">")*V

meanHelper=mu*Helper
stdHelper=std_dev*Helper
SEHelper=SE*Helper
varHelper=var*Helper
cvHelper=cv*Helper
harHelper=har_mu*Helper
geomHelper=geom_mu*Helper
minHelper=mn*Helper
maxHelper=mx*Helper
rngHelper=rng*Helper
g1Helper=g1*Helper
se_g1Helper=se_g1*Helper
g2Helper=g2*Helper
se_g2Helper=se_g2*Helper
medianHelper1 = md * Helper
iqmHelper = iqm * Helper

writeMM(meanHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/mean", format="text");
writeMM(stdHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/std", format="text");
writeMM(SEHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/se", format="text");
writeMM(varHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/var", format="text");
writeMM(cvHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/cv", format="text");
writeMM(harHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/har", format="text");
writeMM(geomHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/geom", format="text");
writeMM(minHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/min", format="text");
writeMM(maxHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/max", format="text");
writeMM(rngHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/rng", format="text");
writeMM(g1Helper, "C:/workspace/DML/test/scripts/applications/univariate/out/g1", format="text");
writeMM(se_g1Helper, "C:/workspace/DML/test/scripts/applications/univariate/out/se_g1", format="text");
writeMM(g2Helper, "C:/workspace/DML/test/scripts/applications/univariate/out/g2", format="text");
writeMM(se_g2Helper, "C:/workspace/DML/test/scripts/applications/univariate/out/se_g2", format="text");
writeMM(out_minus, "C:/workspace/DML/test/scripts/applications/univariate/out/out_minus", format="text");
writeMM(out_plus, "C:/workspace/DML/test/scripts/applications/univariate/out/out_plus", format="text");
writeMM(medianHelper1, "C:/workspace/DML/test/scripts/applications/univariate/out/median", format="text");
writeMM(Q, "C:/workspace/DML/test/scripts/applications/univariate/out/quantile", format="text");
writeMM(iqmHelper, "C:/workspace/DML/test/scripts/applications/univariate/out/iqm", format="text");
