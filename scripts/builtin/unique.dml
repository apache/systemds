unique = function(matrix[double] v)
    return (matrix[double] r) {
    v_sorted = order(target=v, by=1, decreasing=FALSE, index.return=FALSE);
    mask = rbind(matrix("1", rows = 1, cols = 1), v_sorted[1:nrow(v_sorted)-1,] != v_sorted[2:nrow(v_sorted),]);
    r = removeEmpty(target = v_sorted, margin = "rows", select = mask);
}
