rows = $rows;
cols = $cols;
min = ifdef($min, 0);
max = ifdef($max, 1);
use_seed = ifdef($use_seed, FALSE);
seed = ifdef($seed, 13);
target = $target;

if(use_seed)
  X = rand(rows=rows, cols=cols, min=min, max=max, seed=seed);
else
  X = rand(rows=rows, cols=cols, min=min, max=max);

write(X, target);
