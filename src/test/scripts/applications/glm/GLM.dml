X = read($X);
Y = read($Y);
fmtB = ifdef ($fmt, "text");

dfam = ifdef ($dfam, 1);
vpow = ifdef ($vpow, 0.0);
link = ifdef ($link, 0);
lpow = ifdef ($lpow, 1.0);
yneg = ifdef ($yneg, 0.0);
icpt = ifdef ($icpt, 0);
disp = ifdef ($disp, 0.0);
reg = ifdef ($reg, 0.0);
eps = ifdef ($tol, 0.000001);
moi = ifdef ($moi, 200);
mii = ifdef ($mii, 0);

beta_out = glm(X=X, Y=Y, dfam=dfam, vpow=vpow, link=link, lpow=lpow,
  yneg=yneg, intercept=icpt, disp=disp, reg=reg, tol=eps, maxIter=moi, maxInnerIter=mii);

write (beta_out, $B, format=fmtB);

