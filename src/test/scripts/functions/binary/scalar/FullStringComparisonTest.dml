string1 = $1;
string2 = $2;
type = $3;

b = FALSE;
if( type == 0 )
{
   b = (string1 > string2);
}
else if( type == 1 )
{
   b = (string1 < string2)
}
else if( $3 == 2 )
{
   b = (string1 == string2)
}
else if( type == 3 )
{
   b = (string1 != string2)
}
else if( type == 4 )
{
   b = (string1 >= string2)
}
else if( type == 5 )
{
   b = (string1 <= string2)
}

write(b, $4); 