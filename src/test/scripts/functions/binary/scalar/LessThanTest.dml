# junit test class: org.apache.sysml.test.integration.functions.binary.scalar.LessThanTest.java

$$readhelper$$

Left1 = Helper;
i = 0;
while(i < 1)
{
	Left1 = 2 * Left1;
	i = i + 1;
}
write(Left1, "$$outdir$$left_1", format="text");

Left2 = Helper;
i = 0;
while(i < 0)
{
	Left2 = 2 * Left2;
	i = i + 1;
}
write(Left2, "$$outdir$$left_2", format="text");

Left3 = Helper;
i = 0;
while(i < -1)
{
	Left3 = 2 * Left3;
	i = i + 1;
}
write(Left3, "$$outdir$$left_3", format="text");

Right1 = Helper;
i = 0;
while(1 < i)
{
	Right1 = 2 * Right1;
	i = i - 1;
}
write(Right1, "$$outdir$$right_1", format="text");

Right2 = Helper;
i = 0;
while(0 < i)
{
	Right2 = 2 * Right2;
	i = i - 1;
}
write(Right2, "$$outdir$$right_2", format="text");

Right3 = Helper;
i = 0;
while(-1 < i)
{
	Right3 = 2 * Right3;
	i = i - 1;
}
write(Right3, "$$outdir$$right_3", format="text");