tempFile = $1

data = matrix(c(1, 2, 3, 4), 2, 2)
frame1 = as.frame(data)
colNames1 = as.frame(matrix(c("ID", "Value"), 1, 2))

frame1 = setColNames(frame1, colNames1)
retrievedNames = getColNames(frame1)

if (!all(retrievedNames == colNames1[1,])) {
    stop("Name mismatch: Expected " + toString(colNames1[1,]) + " but got " + toString(retrievedNames))
}

write(frame1, tempFile, format="csv", header=TRUE)
frame2 = read(tempFile, format="csv", header=TRUE)

reloadedNames = getColNames(frame2)
if (!all(reloadedNames == colNames1[1,])) {
    stop("CSV reload name mismatch: Expected " + toString(colNames1[1,]) + " but got " + toString(reloadedNames))
}

print("All tests passed successfully!")
