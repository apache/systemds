X = read($1)
gloveMatrix = read($2)
vocab = read($3)
similarity = $4
threshold = as.double($5)

[Y_unique, Y_duplicates] = dedup(X, gloveMatrix, vocab, similarity, threshold)

write(Y_unique, $6, format="binary")
write(Y_duplicates, $7, format="binary")