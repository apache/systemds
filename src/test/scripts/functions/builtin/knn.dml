X = read($in_X)
T = read($in_T)
CL = read($in_CL)
k = $in_k

[NNR, PR, FI] = knn(Train=X,  Test=T, CL=CL, k=k, predictCont=1);

PR_val = matrix(0, 0, ncol(T));
for(i in 1:nrow(T)) {
  PR_val = rbind(PR_val, X[as.scalar(PR[i]), ]);
}

write(NNR, $out_NNR);
write(PR_val, $out_PR);
