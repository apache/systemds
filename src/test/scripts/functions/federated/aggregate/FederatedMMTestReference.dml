if($6) {
  X = rbind(read($1), read($2), read($3), read($4));
  Y = X * 7;
  X = t(X);
}
else {
  X = cbind(read($1), read($2), read($3), read($4));
  Y = t(X) * 7;
}

while(FALSE) { }

s = X %*% Y;
write(s, $5);
