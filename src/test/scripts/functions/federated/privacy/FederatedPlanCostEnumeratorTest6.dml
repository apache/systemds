A = matrix(7,10,10);
b = rand(rows = 1, cols = ncol(A), min = 1, max = 2)
d = sum(b) * 8
i = 0
while(sum(b) < d){
    i = i + 1
    b = b + i
    s = b %*% A
    print(mean(s))
}
