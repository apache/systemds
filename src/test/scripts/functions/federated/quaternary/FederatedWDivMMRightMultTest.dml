X = federated(addresses=list($in_X1, $in_X2),
  ranges=list(list(0, 0), list($rows, $cols), list($rows, 0), list($rows * 2, $cols)))

U = read($in_U)
V = read($in_V)

tmp = (X * (U %*% t(V))) %*% V;

while(FALSE) { }
# verify output is federated (creating fed_uak+ heavy hitter)
sZ = sum(tmp);
while(FALSE) { }
Z = as.matrix(sZ);

write(Z, $out_Z)
