X = read($1);

tmp = X[,1];
for (i in 2:ncol(X)) {
  Res1 = t(tmp) %*% tmp;
  tmp = cbind(tmp, X[,i]);
  #while(FALSE) {}	
  #print(sum(Res1));
  print(toString(Res1));
}

tmp = X[,1];
for (i in 2:ncol(X)) {
  Res1 = t(tmp) %*% tmp;

  if (i > 2) {
  while (FALSE) {}
  #tmp = tmp[,1:(i-2)];
  r1 = cbind((t(tmp[,1:(i-2)]) %*% tmp[,1:(i-2)]), (t(tmp[,1:(i-2)]) %*% X[,i-1]));
  r2 = cbind((t(X[,i-1]) %*% tmp[,1:(i-2)]), (t(X[,i-1]) %*% X[,i-1]));
  Res1 = rbind(r1, r2);
  while(FALSE) {}	
  }

  tmp = cbind(tmp, X[,i]);
  while(FALSE) {}	
  #print(sum(Res1));
  print(toString(Res1));
}

write(Res1, $2, format="text");
