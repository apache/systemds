r = 4
c = 3
X = random.normal(r, c, 1)
print("sum of random.normal matrix is " + sum(X))

Y = random.uniform(r, c, 1, 0, 1)
print("avg of random.uniform matrix is " + avg(Y))

p = norm.cdf(0.4, 0, 1)
print("norm.cdf is " + p)

p = expon.cdf(0.7, 1.0)
print("expon.cdf is " + p)

p = chi.cdf(0.3, 5)
print("chi.cdf is " + p)

p = f.cdf(2, 100, 200)
print("f.cdf is " + p)

p = t.cdf(1, 100)
print("t.cdf is " + p)
