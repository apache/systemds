foo = function(Matrix[Double] A, Integer type) return (Matrix[Double] B) {
  for(i in 1:2) {
    if( type==1 )
      B = A * A * A;
    else
      B = A - 0.1; 
  } 
}

X = matrix(0.1, rows=100, cols=10);
Y = foo(X, 1);
z = as.matrix(sum(Y)*7);

write(z, $1);
