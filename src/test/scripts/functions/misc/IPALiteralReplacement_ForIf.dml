foo = function( Matrix[Double] A ) return( Matrix[Double] B )
{
   for( i in 1:1 ) {
     continue = TRUE;
     if( sum(A)<0 ) {
        continue = FALSE;
     }
     iter = 0;
     if( continue ) {
        iter = iter+1;
     }
     B = A+iter;
   }
}


A = matrix(1, rows=10, cols=10);
R = foo(A);

write(R, $1);