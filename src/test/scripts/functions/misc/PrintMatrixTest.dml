printm = function(string name, matrix[double] M) return (matrix[double] M) {
  print("printing matrix = " + name)
  for (i in 1:nrow(M)) {
    for (j in 1:ncol(M)) {
       e = as.scalar(M[i,j])
       print(" " + i + " " + j + " " + e)
    }
  }
  print("done printing matrix = " + name)
  M = M
}

MM1 = matrix(7, rows=2, cols=2)
str_name = "MM1"

ignore = printm(str_name, MM1)