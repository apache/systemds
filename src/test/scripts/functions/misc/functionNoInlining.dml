foo = function(Double input) return (Double out)
{
   if( 1==1 ){ #prevent inlining
      out = input + input;
   }
}

foo2 = function(Integer input) return (Double out)
{ 
   if( 1==1 ){ #prevent inlining
      out = input + input;
   }
}

x = $1;

if( 1!=1 ){
  x = 7;
}

ret1 = foo(x);
ret2 = foo2(x);
 
print("Result1: "+ret1);
print("Result2: "+ret2);
