e1 = "element1"
modelList = list(e1=e1)
X = matrix(1, rows=2, cols=3)
Y = matrix(2, rows=2, cols=3)
X_val = matrix(3, rows=2, cols=3)
Y_val = matrix(4, rows=2, cols=3)
modelAvg = as.logical($1)

gradients = function (matrix[double] input) return (matrix[double] output) {
  output = input
}

aggregation = function (matrix[double] input) return (matrix[double] output) {
  output = input
}

e2 = "element2"
params = list(e2=e2)

# Use paramserv function
# Remove the optional "val_features" and "val_labels"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", mode="REMOTE_SPARK", utype="BSP", freq="EPOCH", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH", nBatches=1, modelAvg=modelAvg)

# Remove the optional "val_features" and "val_labels" with freq="nBatches"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", mode="REMOTE_SPARK", utype="BSP", freq="NBATCHES", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH", nBatches=16, modelAvg=modelAvg)


# Remove the optional "mode" with freq="nBatches"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", utype="BSP", freq="NBATCHES", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH",nBatches=16, modelAvg=modelAvg)

# Remove the optional "mode"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", utype="BSP", freq="EPOCH", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH",nBatches=1, modelAvg=modelAvg)

# Remove the optional "utype"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", epochs=100, freq="EPOCH", batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH",nBatches=1, modelAvg=modelAvg)

# Remove the optional "freq"
modelList2 = paramserv(model=modelList, features=X, labels=Y, upd="gradients", agg="aggregation", utype="BSP", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH",nBatches=1, modelAvg=modelAvg)

# Remove the optional "hyperparams"
modelList2 = paramserv(model=modelList, features=X, labels=Y, val_features=X_val, val_labels=Y_val, upd="gradients", agg="aggregation", mode="REMOTE_SPARK", utype="BSP", freq="EPOCH", epochs=100, batchsize=64, k=7, scheme="DISJOINT_ROUND_ROBIN", checkpointing="EPOCH",nBatches=1, modelAvg=modelAvg)

# Remove the optional "batchsize"
modelList3 = paramserv(model=modelList, features=X, labels=Y, val_features=X_val, val_labels=Y_val, upd="gradients", agg="aggregation", mode="LOCAL", utype="ASP", freq="BATCH", epochs=100, k=7, scheme="DISJOINT_RANDOM", hyperparams=params, checkpointing="NONE",nBatches=1, modelAvg=modelAvg)

# Remove the optional "checkpointing"
modelList4 = paramserv(model=modelList, features=X, labels=Y, val_features=X_val, val_labels=Y_val, upd="gradients", agg="aggregation", mode="LOCAL", utype="SSP", freq="EPOCH", batchsize=64, epochs=100, k=7, scheme="OVERLAP_RESHUFFLE", hyperparams=params,nBatches=1, modelAvg=modelAvg)