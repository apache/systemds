  maxii = 20
  verbose = FALSE
  columnId = -1
  Y = read($Y)
  X = federated(addresses=list($X1, $X2),
    ranges=list(list(0, 0), list($r / 2, $c), list($r / 2, 0), list($r, $c)))
  intercept = FALSE
  epsilon = 1e-12
  reg = 1
  maxIterations = 100

  model = l2svm(X=X,  Y=Y, intercept = FALSE, tol = epsilon, reg = reg, maxIter = maxIterations)

  write(model, $Z)
