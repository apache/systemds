X = federated(addresses=list($X1, $X2),
              ranges=list(list(0, 0), list($r / 2, $c), list($r / 2, 0), list($r, $c)))
Y = federated(addresses=list($Y1, $Y2),
              ranges=list(list(0, 0), list($r/2, $c), list($r / 2, 0), list($r, $c)))
W = rand(rows=$r, cols=$c, min=0, max=1, pdf='uniform', seed=5)
s = 3.5
Z0 = W + s * X
Z1 = 1 - Y * Z0
Z = sum(Z1)
write(Z, $Z)
