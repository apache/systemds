n = 2;
m = 2;

parfor (j in 1:m, log=DEBUG){
  A = matrix(3, rows=n, cols=3);
  B = matrix(2, rows=n, cols=3);
  for (i in 1:n){
  		print("i = " + i + " j = " + j + " Sum(B) = " + sum(B));
		A[i,1] = j*2+i;
		if(1 == 1)
			B = A
		A[i,2] = j*3+i;
		A[i,3] = j*4+i;
	}
}
