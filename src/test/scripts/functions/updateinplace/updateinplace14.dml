n = 2;
m = 2;

parfor (j in 1:m, log=DEBUG){
  A = matrix(3, rows=n, cols=1);
  B = matrix(2, rows=n, cols=1);
  i = 1
  while (i <= n){
		test = testUIP(B)
		A[i,1] = j*7+i;
		B[i,1] = j*3+i;
		i = i + 1
	}
	
  print("Sum from testUIP = " + test);
  print("Sum(B) = " + sum(B));
}


testUIP = function (matrix[double] in_m_data)
	return (double sumInData) {
        if(1 == 1) {}
	sumInData = sum(in_m_data);
}
