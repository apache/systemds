n = 2;
m = 2;

parfor (j in 1:m, log=DEBUG){
  A = matrix(3, rows=n, cols=n);
  B = matrix(2, rows=n, cols=n);
  C = matrix(1, rows=n, cols=n);
  D = matrix(1, rows=n, cols=n);
  i = 1
  while (i <= n){
  	print("i = " + i + " j = " + j + " Sum(D) = " + sum(D));
	if(1 == 1) {
		D = C %*% (t(A) * 7) 
	}
	A[i,1] = j*7+i;
	i = i + 1
  }
}
